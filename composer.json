{
  "name": "payeye/prestashop",
  "description": "PayEye Payments",
  "scripts": {
    "phpstan": "_PS_ROOT_DIR_=.  vendor/bin/phpstan analyse --configuration=tests/phpstan/phpstan.neon",
    "cs-fix": "php vendor/bin/php-cs-fixer fix"
  },
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\PayEye\\": "src/"
    },
    "classmap": [
      "classes",
      "payeye.php",
      "controllers"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PayEye\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "prepend-autoloader": false
  },
  "type": "prestashop-module",
  "author": "PayEye",
  "require-dev": {
    "ext-curl": "*",
    "phpstan/phpstan": "^1.9",
    "phpunit/phpunit": "^9.6.3",
    "prestashop/autoindex": "^2.1",
    "prestashop/php-dev-tools": "^4.3",
    "symfony/var-dumper": "^5.4"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/52451105/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "ext-json": "*",
    "chillerlan/php-qrcode": "^3.4.1",
    "payeye/lib": "1.18.12",
    "ext-http": "*"
  },
  "archive": {
    "exclude": [
      "build-local.sh",
      ".php-cs-fixer.cache",
      ".php-cs-fixer.dist.php",
      ".DS_Store",
      ".idea",
      "config",
      "node_modules",
      "scripts",
      "scss",
      "tests",
      ".env",
      ".env.smaple",
      ".gitignore",
      ".phpunit.result.cache",
      "config.json",
      "composer.json",
      "composer.lock",
      "movefile.yml",
      "package.json",
      "phpunit.xml",
      "README.md",
      "webpack.config.js",
      "yarn.lock",
      "devops",
      ".gitlab-ci.yml",
      "auth.json",
      "auth.json.dist",
      "tsconfig.json",
      "dist/*.map",
      "languages/*.po~",
      "languages/*.pot~",
      "languages/.gitkeep"
    ]
  }
}
